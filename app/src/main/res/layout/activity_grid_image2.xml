<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GridImage2">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:onClick="onGridClicked"
        android:transitionName="@string/shared_el_transition_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/b2" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView10"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:onClick="onGridClicked"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/b5" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView11"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:onClick="onGridClicked"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/b1"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView12"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:onClick="onGridClicked"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/dhaka" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView13"
        android:layout_width="140dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:onClick="onGridClicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/b2" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView14"
        android:layout_width="140dp"
        android:layout_height="150dp"
        android:scaleType="fitXY"
        android:onClick="onGridClicked"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/b4" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView15"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:onClick="onGridClicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:srcCompat="@drawable/b3" />

    <ImageView
        android:transitionName="@string/shared_el_transition_name"
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:scaleType="centerCrop"
        android:onClick="onGridClicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15"
        app:srcCompat="@drawable/b1" />
</androidx.constraintlayout.widget.ConstraintLayout>